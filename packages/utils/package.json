{
  "name": "@pixi/utils",
  "version": "6.4.2",
  "main": "dist/cjs/utils.js",
  "module": "dist/esm/utils.js",
  "bundle": "dist/browser/utils.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./dist/esm/utils.js"
