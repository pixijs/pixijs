{
  "name": "@pixi/graphics-extras",
  "version": "7.0.0-beta.3",
  "main": "dist/cjs/graphics-extras.js",
  "module": "dist/esm/graphics-extras.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
