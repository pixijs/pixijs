  },
  "files": [
    "lib",
    "dist",
    "*.d.ts"
  ],
  "peerDependencies": {
    "@pixi/core": "6.5.0-rc",
    "@pixi/display": "6.5.0-rc",
    "@pixi/graphics": "6.5.0-rc",
    "@pixi/settings": "6.5.0-rc",
    "@pixi/text": "6.5.0-rc",
