{
  "name": "@pixi/prepare",
  "version": "6.5.0-rc",
  "main": "dist/cjs/prepare.js",
  "module": "dist/esm/prepare.js",
  "bundle": "dist/browser/prepare.js",
  "types": "index.d.ts",
