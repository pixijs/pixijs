{
  "name": "@pixi/compressed-textures",
  "version": "6.3.2",
  "description": "Loaders for compressed texture file formats",
  "keywords": [
    "pixi.js",
    "pixi",
    "loaders",
    "ktx",
    "dds",
    "compressed-textures"
  ],
  "author": "Shukant Pal <shukantpal@outlook.com>",
  "homepage": "https://github.com/pixijs/pixi.js#readme",
  "license": "MIT",
  "main": "dist/cjs/compressed-textures.js",
  "module": "dist/esm/compressed-textures.js",
  "bundle": "dist/browser/compressed-textures.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
          "default": "./dist/cjs/compressed-textures.js",
          "types": "./index.d.ts"
      },
      "require": {
          "default": "./dist/esm/compressed-textures.js",
          "types": "./index.d.ts"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi.js.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "sideEffects": true,
  "bugs": {
    "url": "https://github.com/pixijs/pixi.js/issues"
  },
  "peerDependencies": {
    "@pixi/constants": "6.3.2",
    "@pixi/core": "6.3.2",
    "@pixi/loaders": "6.3.2",
    "@pixi/settings": "6.3.2",
    "@pixi/utils": "6.3.2"
  }
}
