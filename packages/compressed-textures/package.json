{
  "name": "@pixi/compressed-textures",
  "version": "7.1.0-alpha",
  "description": "Loaders for compressed texture file formats",
  "keywords": [
    "pixi.js",
    "pixi",
    "loaders",
    "ktx",
    "dds",
    "compressed-textures"
  ],
  "author": "Shukant Pal <shukantpal@outlook.com>",
  "homepage": "https://github.com/pixijs/pixi.js#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixijs.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "sideEffects": true,
  "bugs": {
    "url": "https://github.com/pixijs/pixijs/issues"
  },
  "pixiRequirements": [
    "@pixi/assets",
    "@pixi/core"
  ]
}
