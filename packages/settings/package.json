  "contributors": [
    "Matt Karl <matt@mattkarl.com>"
  ],
  "homepage": "http://pixijs.com/",
  "bugs": "https://github.com/pixijs/pixi.js/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixi.js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "dependencies": {
    "@pixi/constants": "7.0.0-beta"
  },
  "devDependencies": {
    "ismobilejs": "^1.1.0"
  }
