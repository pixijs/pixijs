{
    "source": {
        "include": [
            "bundles",
            "packages"
        ],
        "excludePattern": "(node_modules|lib|dist|test|deprecated|canvas)"
    },
    "plugins": [
        "@pixi/jsdoc-template/plugins/es6-fix"
    ],
    "opts": {
        "recurse": true,
        "destination": "./dist/types",
        "outFile": "pixi.js.d.ts",
        "template": "./node_modules/tsd-jsdoc/dist"
    }
}
