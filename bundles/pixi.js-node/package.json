{
  "name": "@pixi/node",
  "version": "7.1.0-alpha",
  "description": "Bundle for PixiJS with support for NodeJS",
  "homepage": "http://pixijs.com/",
  "bugs": "https://github.com/pixijs/pixijs/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixijs.git"
  },
  "license": "MIT",
  "author": "PixiJS Team",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test:import": "node test/import.mjs",
    "test:require": "node test/require.cjs",
    "test:dist": "run-s test:import test:require",
    "test": "npx jest --silent",
    "coverage": "npm run test -- --coverage --maxWorkers=4"
  },
  "dependencies": {
    "@pixi/app": "7.1.0-alpha",
    "@pixi/assets": "7.1.0-alpha",
    "@pixi/core": "7.1.0-alpha",
    "@pixi/display": "7.1.0-alpha",
    "@pixi/extensions": "7.1.0-alpha",
    "@pixi/extract": "7.1.0-alpha",
    "@pixi/filter-alpha": "7.1.0-alpha",
    "@pixi/filter-blur": "7.1.0-alpha",
    "@pixi/filter-color-matrix": "7.1.0-alpha",
    "@pixi/filter-displacement": "7.1.0-alpha",
    "@pixi/filter-fxaa": "7.1.0-alpha",
    "@pixi/filter-noise": "7.1.0-alpha",
    "@pixi/graphics": "7.1.0-alpha",
    "@pixi/mesh": "7.1.0-alpha",
    "@pixi/mesh-extras": "7.1.0-alpha",
    "@pixi/mixin-cache-as-bitmap": "7.1.0-alpha",
    "@pixi/mixin-get-child-by-name": "7.1.0-alpha",
    "@pixi/mixin-get-global-position": "7.1.0-alpha",
    "@pixi/particle-container": "7.1.0-alpha",
    "@pixi/prepare": "7.1.0-alpha",
    "@pixi/sprite": "7.1.0-alpha",
    "@pixi/sprite-animated": "7.1.0-alpha",
    "@pixi/sprite-tiling": "7.1.0-alpha",
    "@pixi/spritesheet": "7.1.0-alpha",
    "@pixi/text": "7.1.0-alpha",
    "@pixi/text-bitmap": "7.1.0-alpha",
    "@types/gl": "^6.0.2",
    "@types/xml2js": "^0.4.11",
    "canvas": "^2.9.1",
    "cross-fetch": "^3.1.5",
    "gl": "^6.0.1",
    "xml2js": "^0.4.23"
  },
  "nodeDependencies": [
    "fs",
    "path"
  ],
  "publishConfig": {
    "access": "public"
  }
}
