{
  "name": "pixi.js-legacy",
  "description": "The HTML5 Creation Engine: Create beautiful digital content with the fastest, most flexible 2D WebGL renderer.",
  "author": "Mat Groves",
  "contributors": [
    "Ivan Popelyshev <ivan.popelyshev@gmail.com>",
    "Matt Karl <matt@mattkarl.com>",
    "Chad Engler <chad@pantherdev.com>",
    "Richard Davey <rdavey@gmail.com>"
  ],
  "standalone": true,
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "bundle": "dist/pixi-legacy.js",
  "bundleModule": "dist/pixi-legacy.mjs",
  "bundleOutput": {
    "name": "PIXI"
  },
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "homepage": "http://www.pixijs.com/",
  "bugs": "https://github.com/pixijs/pixijs/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixijs.git"
  },
  "files": [
    "lib",
    "dist",
    "*.d.ts"
  ],
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/pixijs"
  },
  "dependencies": {
    "@pixi/canvas-display": "file:../../packages/canvas-display",
    "@pixi/canvas-extract": "file:../../packages/canvas-extract",
    "@pixi/canvas-graphics": "file:../../packages/canvas-graphics",
    "@pixi/canvas-mesh": "file:../../packages/canvas-mesh",
    "@pixi/canvas-particle-container": "file:../../packages/canvas-particle-container",
    "@pixi/canvas-prepare": "file:../../packages/canvas-prepare",
    "@pixi/canvas-renderer": "file:../../packages/canvas-renderer",
    "@pixi/canvas-sprite": "file:../../packages/canvas-sprite",
    "@pixi/canvas-sprite-tiling": "file:../../packages/canvas-sprite-tiling",
    "@pixi/canvas-text": "file:../../packages/canvas-text",
    "pixi.js": "file:../pixi.js"
  }
}
